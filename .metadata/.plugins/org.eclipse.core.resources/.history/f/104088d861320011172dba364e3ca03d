
public class Debug {
int valor1,valor2, valor3;
String s1, s2, s3;
boolean bo1,bo2,bo3;
double do1,do2,do3;
	
Debug (int valor1, int valor2){
	this.valor1 = valor1;
	this.valor2 = valor2;
Prompt1(valor1,valor2);	
}
Debug(int valor1){
	this.valor1 = valor1;
	Prompt2(valor1);
}

Debug(double do1, double do2){
	this.do1 = do1;
	this.do2 = do2;
}

Debug(String s1 , String s2){
	this.s1 = s1;
	this.s2 = s2;
	Prompt3(s1, s2);
}
/**Prompt para imprimir dos enteros
 */
static void Prompt1 (int valor1, int valor2){
		
		System.out.println("El valor de valor1 = " +valor1 );
		System.out.println("El valor de valor2 = " +valor2 );		
	}
/** Prompt para imprimir un numero entero
 */
static void Prompt2 (int valor1){
	
	System.out.println("El valor entero es = " +valor1);
}

/** Prompt para imprimir dos String s1 y s2
 */
static void Prompt3 (String s1, String s2){
	
	System.out.println("El valor de cadena s1 es  = " +s1);
	System.out.println("El valor de cadena s2 es  = " +s2);

}

/** Prompt para imprimir dos valores double do1 y do2
 */
static void Prompt4 (double do1, double do2){
	
	System.out.println("El valor de double 1 = " +do1 );
	System.out.println("El valor de double 2 = " +do2 );		
}





}


public class Ordenar {

	//  Diseñar un programa que muestre el producto de los 10 primeros números impares
	
	public void numeros (){
		int multi = 1;
		for(byte numero = 1; numero < 21;numero++){
			
			if(numero % 2 != 0){
				
				multi *= numero; 
			}
			
		}
		
		
	}
	
	
	
	public static void main(String[] args ){
		
		
		
		
		
	}
	
	
	
	
	
}
